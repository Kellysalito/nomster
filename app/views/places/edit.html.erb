<div class="booyah-box col-10 offset-1">
<h1>Edit Your Place</h1>
<%= simple_form_for @place do |f| %>
<%= f.input :name %>
<%= f.input :address %>
<%= f.input :description %>
<br />
<%= f.submit 'Update', class: 'btn btn-primary' %>
<% end %>
</div>

<div class="booyah-box col-10 offset-1">
<h1>Upload a Picture</h1>
<% simple_form_for @photo, :html =>{multipart => true} do |f| %>
<%= f.input :picture %>
<%= f.submit "Upload a Picture", class: 'btn btn-primary' %>
<% end %>

<% end %>